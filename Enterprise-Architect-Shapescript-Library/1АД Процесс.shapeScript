shape main{	layouttype="border";	v_align="center";	h_align="center";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		defsize(90,45);		startpath();		moveto(0,0);		lineto(75,0);		lineto(75,-30);		lineto(100,50);		lineto(75,130);		lineto(75,100);		lineto(0,100);		lineto(0,0);		endpath();		fillandstrokepath();		if(hastag("Погружение","Подпроцесс"))		{			startpath();			moveto(65,60);			lineto(85,60);			lineto(85,40);			lineto(100,85);			lineto(85,130);			lineto(85,100);			lineto(65,100);			lineto(65,60);			endpath();			fillandstrokepath();			}		if(hastag("Погружение","Операция"))		{			ellipse(66,50, 100,130); // Круг операции		}	}	else	{		// Прямоугольная нотация		defsize(100,70);		roundrect(0,0, 100,100, 20,20); // Прямоугольник с закругленными углами		addsubshape("padding","n");  // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}		shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration process{	orientation="ne";		if(hasproperty("rectanglenotation","0"))	{	}	else	{		// Внимание!!! Декорация «Процесс» сжата в прямогоульнике!!! Не чистое копирование		moveto(0,25);		lineto(55,25);		lineto(55,0);		lineto(80,50);		lineto(55,100);		lineto(55,75);		lineto(0,75);		lineto(0,25);		if(hastag("Погружение","Подпроцесс"))		{			startpath();			moveto(33,50);			lineto(66,50);			lineto(66,40);			lineto(80,70);			lineto(66,100);			lineto(66,90);			lineto(33,90);			lineto(33,50);			endpath();			fillandstrokepath();		}			if(hastag("Погружение","Операция"))		{			ellipse(40,60, 80,100); // Круг операции		}	}}decoration composite{	orientation="SE";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40,40,60);		ellipse(60,40,100,60);		moveto(30,50);		lineto(70,50);	}}