shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		if(hastag("Погружение","—"))		{			defSize(50,25); 			rectangle(0,0, 100,100);			// Пузырек			setfillcolor(190,255,0);			startpath();			arcto(20,-10, 80,50, 0,-24, 100,-24);			endpath();			fillandstrokepath();		}		if(hastag("Погружение","Уровень федеральный"))		{			defSize(50,50); 			rectangle(0,0, 100,50);			// Пузырек			setfillcolor(190,255,0);			startpath();			moveto(25,0);			arcto(20,-5, 80,25, 0,-32, 100,-32); 			endpath();			fillandstrokepath();			setpenwidth(2);			moveto(0,75); // Линия снизу			lineto(100, 75);		}		if(hastag("Погружение","Уровень региональный"))		{			defSize(50,50); 			rectangle(0,25, 100,75);			// Пузырек			setFillColor(190,255,0);			startpath();			moveto(25,25);			arcto(20,20, 80,50, 0,-7, 100,-7);			endpath();			fillandstrokepath();			setpenwidth(2);			moveto(0,0); // Линия сверху			lineto(100, 0);			moveto(0,100); // Линия снизу			lineto(100, 100);		}		if(hastag("Погружение","Уровень самоуправления"))		{			defSize(50,50); 			rectangle(0,100, 100,50);			// Пузырек			setfillcolor(190,255,0);			startpath();			moveto(25,50);			arcto(20,45, 80,75, 0,18, 100,18);			endpath(); // End tracing out a path			fillandstrokepath();			setpenwidth(2);			moveto(0,0); // Линия сверху			lineto(100, 0);		}	}	else	{		// Прямоугольная нотация		defsize(100,70);		startpath(); // Восьмиугольник		moveto(10,0);		lineto(90,0);		lineto(100,20);		lineto(100,80);		lineto(90,100);		lineto(10,100);		lineto(0,80);		lineto(0,20);		lineto(10,0);		endpath();		fillandstrokepath();				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration self_control{	orientation="ne";	setorigin("ne", -25,0); // Смещение декорации для восьмиугольника		if(hasproperty("rectanglenotation","0"))	{	}	else	{		// Декарация «Уровень» не является полной копией		if(hastag("Погружение","—"))		{			rectangle(0,0, 100,100);			// Пузырек			setfillcolor(190,255,0);			startpath();			arcto(20,-10, 80,50, 0,-24, 100,-24);			endpath();			fillandstrokepath();		}		if(hastag("Погружение","Уровень федеральный"))		{			rectangle(0,0, 100,50);			// Пузырек			setfillcolor(190,255,0);			startpath();			moveto(25,0);			arcto(20,-5, 80,25, 0,-32, 100,-32); 			endpath();			fillandstrokepath();			setpenwidth(2);			moveto(0,75); // Лини€ снизу			lineto(100, 75);		}		if(hastag("Погружение","Уровень региональный"))		{			rectangle(0,25, 100,75);			// Пузырек			setFillColor(190,255,0);			startpath();			moveto(25,25);			arcto(20,20, 80,50, 0,-7, 100,-7);			endpath();			fillandstrokepath();			setpenwidth(2);			moveto(0,0); // Линия сверху			lineto(100, 0);			moveto(0,100); // Линия снизу			lineto(100, 100);		}		if(hastag("Погружение","Уровень самоуправления"))		{			rectangle(0,100, 100,50);			// Пузырек			setfillcolor(190,255,0);			startpath();			moveto(25,50);			arcto(20,45, 80,75, 0,18, 100,18);			endpath();			fillandstrokepath();			setpenwidth(2);			moveto(0,0); // Линия сверху			lineto(100, 0);		}	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		// Три звена цепочки		ellipse(0,40, 40,60);		ellipse(60,40, 100,60);		moveto(30,50);		lineto(70,50);	}}