shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		if(hastag("Погружение","Результат"))		{			defsize(50,50);			ellipse(0,0, 100,100);			ellipse(15,15, 85,85);			ellipse(30,30, 70,70);			setfillcolor(getuserbordercolor());			ellipse(45,45, 55,55);			setfillcolor(getuserfillcolor());			// arrow			moveto(50,50);			lineto(100,0);			// feathers			moveto(95,-5);			lineto(90,10);			lineto(105,5);		}		if(hastag("Погружение","Ключевой показатель"))		{			defsize(50,50);			startpath(); // Общий контур и заливка			moveto(50,100);			lineto(0,100);			lineto(0,20);			lineto(-20,20);			lineto(25,-25);			lineto(50,0);			lineto(100,0);			lineto(100,75);			lineto(125,75);			lineto(75,125);			lineto(50,100);			endpath();			fillandstrokepath();			moveto(50,0);			lineto(75,25);			lineto(50,25);			lineto(50,75);			lineto(25,75);			lineto(50,100);		}	}	else	{		// Прямоугольная нотация		defsize(100,70);		startpath(); // Восьмиугольник		moveto(10,0);		lineto(90,0);		lineto(100,20);		lineto(100,80);		lineto(90,100);		lineto(10,100);		lineto(0,80);		lineto(0,20);		lineto(10,0);		endpath();		fillandstrokepath();				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration outcome{	orientation="ne";	SetOrigin("ne",-25,0); // Смещение декорации для восьмиугольника		if(hasproperty("rectanglenotation","0"))	{	}	else	{		if(hastag("Погружение","Результат"))		{			ellipse(0,0, 100,100);			ellipse(15,15, 85,85);			ellipse(30,30, 70,70);			setfillcolor(getuserbordercolor());			ellipse(45,45, 55,55);			setfillcolor(getuserfillcolor());			// arrow			moveto(50,50);			lineto(100,0);			// feathers			moveto(95,-5);			lineto(90,10);			lineto(105,5);		}		if(hastag("Погружение","Ключевой показатель"))		{			startpath(); // Общий контур и заливка			moveto(50,80);			lineto(15,80);			lineto(15,30);			lineto(0,30);			lineto(30,0);			lineto(50,20);			lineto(85,20);			lineto(85,70);			lineto(100,70);			lineto(70,100);			lineto(50,80);			endpath();			fillandstrokepath();			moveto(50,20);			lineto(65,30);			lineto(50,30);			lineto(50,70);			lineto(35,70);			lineto(50,80);		}	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		// Три звена цепочки		ellipse(0,40,40,60);		ellipse(60,40,100,60);		moveto(30,50);		lineto(70,50);	}}