shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		defsize(70,75);		startpath();		moveto(0,20);		lineto(50,0);		lineto(100,20);		lineto(100,100);		lineto(50,80);		lineto(0,100);		lineto(0,20);		endpath();		fillandstrokepath();		if(hastag("Погружение","Подфункция"))		{			startpath();			moveto(75,60);			lineto(95,50);			lineto(115,60);			lineto(115,100);			lineto(95,90);			lineto(75,100);			lineto(75,60);			endpath();			fillandstrokepath();		}	}	else	{		// Прямоугольная нотация		defsize(100,70);		roundrect(0,0, 100,100, 20,20); // Прямоугольник с закругленными углами				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration function{	orientation="ne";		if(hasproperty("rectanglenotation","0"))	{	}	else	{		// Внимание!!! Декорация сжата в прямогоульнике!!! Не чистое копирование		moveto(0,20);		lineto(40,0);		lineto(80,20);		lineto(80,100);		lineto(40,80);		lineto(0,100);		lineto(0,20);		if(hastag("Погружение","Подфункция"))		{			startpath();			moveto(40,60);			lineto(60,50);			lineto(80,60);			lineto(80,100);			lineto(60,90);			lineto(40,100);			lineto(40,60);			endpath();			fillandstrokepath();		}	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40,40,60);		ellipse(60,40,100,60);		moveto(30,50);		lineto(70,50);	}}