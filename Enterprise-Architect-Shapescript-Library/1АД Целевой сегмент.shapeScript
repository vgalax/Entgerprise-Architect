shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		defsize(50,50);		setpenwidth(1);		startpath();		moveto(0,25);		arcto(33,0, 66,33, 63,25, 36,25); // Верхний сегмент		lineto(100,25);		lineto(100,35);		arcto(75,33, 108,66, 100,35, 100,63); // Правый сегмент		lineto(100,75);		lineto(60,75);		arcto(33,66, 66,100, 34,75, 65,75); // Нижний сегмент		lineto(0,75);		lineto(0,63);		arcto(-8,33, 33,66, 0,66, 0,33); // Левый сегмент		endpath();		fillandstrokepath();		setpencolor(getuserfillcolor());		setpenwidth(2);		moveto(35,25);		lineto(63,25);		moveto(35,75);		lineto(63,75);	}	else	{		// Прямоугольная нотация		defsize(100,70);		rectangle(0,0, 100,100); // Чистый прямоугольник		addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration goal_segment{	orientation="ne";//	setorigin("ne", -25,5); // Смещение декорации для прямоугольника не выполняется!		if(hasproperty("rectanglenotation","0"))	{	}	else	{		setpenwidth(1);		startpath();		moveto(0,25);		arcto(33,0, 66,33, 63,25, 36,25); // Верхний сегмент		lineto(100,25);		lineto(100,35);		arcto(75,33, 108,66, 100,35, 100,63); // Правый сегмент		lineto(100,75);		lineto(60,75);		arcto(33,66, 66,100, 34,75, 65,75); // Нижний сегмент		lineto(0,75);		lineto(0,63);		arcto(-8,33, 33,66, 0,66, 0,33); // Левый сегмент		endpath();		fillandstrokepath();		setpencolor(getuserfillcolor());		setpenwidth(2);		moveto(35,25);		lineto(63,25);		moveto(35,75);		lineto(63,75);	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		// Три звена цепочки		ellipse(0,40, 40,60);		ellipse(60,40, 100,60);		moveto(30,50);		lineto(70,50);	}}