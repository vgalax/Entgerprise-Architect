shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		defsize(50,50);		ellipse(0,0, 100,100);		moveto(0,50); 		lineto(-30,50);		moveto(25, 25);  // Буква U		lineto(25, 55);		arcto(27,30, 77,80, 25,55, 75,55);		moveto(75, 55);		lineto(75, 25);	}	else	{		// Прямоугольная нотация		defsize(100,70);		rectangle(0,0, 100,100);				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration application_interface{	orientation="ne";	if(hasproperty("rectanglenotation","0"))	{	}	else	{		ellipse(20,10, 100,90);		moveto(0,50);		lineto(20,50);		moveto(40, 30);  // Буква U		lineto(40, 55);		arcto(40,30, 82,80, 40,55, 82,55);		moveto(80, 55);		lineto(80, 30);	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40, 40,60);		ellipse(60,40, 100,60);		moveto(30,50);		lineto(70,50);	}}