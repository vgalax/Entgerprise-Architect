shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		defsize(90,45);		startpath();		moveto(15,0);		lineto(100,0);		lineto(85,100);		lineto(0,100);		lineto(15,0);		endpath();		fillandstrokepath();	}	else	{		// Прямоугольная нотация		defsize(100,70);		roundrect(0,0, 100,100, 20,20); // Прямоугольник с закругленными углами				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration channel{	orientation="ne";		if(hasproperty("rectanglenotation","0"))	{	}	else	{		// Внимание!!! Декорация сжата в прямогоульнике!!! Не чистое копирование		moveto(15,20);		lineto(100,20);		lineto(85,80);		lineto(0,80);		lineto(15,20);	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40,40,60);		ellipse(60,40,100,60);		moveto(30,50);		lineto(70,50);	}}