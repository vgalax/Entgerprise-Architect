shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		defsize(90,45);		rectangle(0,0, 100,100);		rectangle(0,0, 5,100);	}	else	{		// Прямоугольная нотация		defsize(100,70);		startpath(); // Восьмиугольник		moveto(10,0);		lineto(90,0);		lineto(100,20);		lineto(100,80);		lineto(90,100);		lineto(10,100);		lineto(0,80);		lineto(0,20);		lineto(10,0);		endpath();		fillandstrokepath();				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration requirement{	orientation="ne";	setorigin("ne",-25,0); // Смещение декорации для восьмиугольника		if(hasproperty("rectanglenotation","0"))	{	}	else	{		rectangle(0,20, 100,80);		rectangle(0,20, 15,80);	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		// Три звена цепочки		ellipse(0,40,40,60);		ellipse(60,40,100,60);		moveto(30,50);		lineto(70,50);	}}