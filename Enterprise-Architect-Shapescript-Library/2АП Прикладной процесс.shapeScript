shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконка		defsize(90,40);		startpath();		moveto(0,0);		lineto(75,0);		lineto(75,-30);		lineto(100,50);		lineto(75,130);		lineto(75,100);		lineto(0,100);		lineto(0,0);		endpath();		fillandstrokepath();	}	else	{		// Прямоугольник		defsize(105,70);		startpath();		roundrect(0,0,100,100,20,20);		endpath();		fillandstrokepath();				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration application_process{	orientation="ne";	if(hasproperty("rectanglenotation","0"))	{	}	else	{		moveto(0,25);		lineto(55,25);		lineto(55,0);		lineto(80,50);		lineto(55,100);		lineto(55,75);		lineto(0,75);		lineto(0,25);	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40, 40,60);		ellipse(60,40, 100,60);		moveto(30,50);		lineto(70,50);	}}