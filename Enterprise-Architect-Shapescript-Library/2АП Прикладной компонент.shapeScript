shape main{	layouttype="border";	noshadow=true;		if(hasproperty("rectanglenotation","0"))	{		// Иконочная нотация		if(hastag("Вид","Комплекс информационных систем"))		{			defsize(50,40);			rectangle(0,0, 75,75);			rectangle(-10,15, 10,25);			rectangle(-10,50, 10,60);			rectangle(25,25, 100,100);			rectangle(15,40, 35,50);			rectangle(15,75, 35,85);		}		if(hastag("Вид","Информационная система"))		{			defsize(50,40);			rectangle(0,0, 100,100);			rectangle(-15,20, 15,35);			rectangle(-15,65, 15,80);			rectangle(60,70, 110,110);			rectangle(55,80, 65,90);			rectangle(55,90, 65,100);		}		if(hastag("Вид","Прикладной компонент"))		{			defsize(50,40);			rectangle(0,0, 100,100);			rectangle(-15,20, 15,35);			rectangle(-15,65, 15,80);		}		if(hastag("Вид","Прикладная подсистема"))		{			defsize(50,40);			rectangle(0,0, 100,100);			rectangle(-15,20, 15,35);			rectangle(-15,65, 15,80);			rectangle(60,70, 110,110);			rectangle(60,70, 70,110);		}		if(hastag("Вид","Прикладной модуль"))		{			defsize(50,40);			rectangle(0,0, 100,100);			rectangle(-15,20, 15,35);			rectangle(-15,65, 15,80);			rectangle(60,70, 110,110);		}	}	else	{		// Прямоугольная нотация		defsize(100,70);		rectangle(0,0, 100,100);				addsubshape("padding","n"); // Создание отступа для декорации		addsubshape("name","center");				shape padding		{			preferredheight=20;		}				shape name		{			h_align="center";			print("#name#");		}	}}shape label{	setorigin("sw",0,0);	if(hasproperty("rectanglenotation","0"))	{		print("#name#");	}}decoration application_component{	orientation="ne";	if(hasproperty("rectanglenotation","0"))	{	}	else	{		if(hastag("Вид","Комплекс информационных систем"))		{			rectangle(10,0, 80,75);			rectangle(0,15, 20,25);			rectangle(0,50, 20,60);			rectangle(30,25, 100,100);			rectangle(25,40, 45,50);			rectangle(25,75, 45,85);		}		if(hastag("Вид","Информационная система"))		{			rectangle(20,0, 80,80);			rectangle(0,10, 40,30);			rectangle(0,50, 40,70);			rectangle(50,60, 100,100);			rectangle(40,70, 60,75);			rectangle(40,95, 60,90);		}		if(hastag("Вид","Прикладной компонент"))		{			rectangle(20,10, 100,90);			rectangle(0,25, 40,35);			rectangle(0,65, 40,75);		}		if(hastag("Вид","Прикладная подсистема"))		{			rectangle(20,0, 80,80);			rectangle(0,10, 40,30);			rectangle(0,50, 40,70);			rectangle(50,60, 100,100);			rectangle(50,60, 70,100);		}		if(hastag("Вид","Прикладной модуль"))		{			rectangle(20,0, 80,80);			rectangle(0,10, 40,30);			rectangle(0,50, 40,70);			rectangle(50,60, 100,100);		}	}}decoration composite{	orientation="se";	if(hasproperty("iscomposite","true"))	{		ellipse(0,40, 40,60);		ellipse(60,40, 100,60);		moveto(30,50);		lineto(70,50);	}}